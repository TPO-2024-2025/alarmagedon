{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2020",
    "moduleResolution": "Node",
    "lib": ["DOM", "ES2021"],
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["./*.ts"]
}
